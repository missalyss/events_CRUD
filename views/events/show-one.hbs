<h1>This event:</h1>
{{#each oneEvent}}
<h2>{{title}}</h2>
<p>{{description}}</p>
{{#if over_21}} <p>This is a 21+ event.</p> {{/if}}
<p>{{start_datetime}} - {{end_datetime}}</p>

<p>Venue: {{name}}</p>
<p>{{line_1}} {{line_2}}</p>
<p>{{city}} {{zip}}</p>
</br>
<p>Tickets available: {{capacity}}</p>

<a href="#">Register for this event</a>
{{/each}}
